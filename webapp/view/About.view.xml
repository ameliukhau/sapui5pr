<mvc:View
	controllerName="opensap.myapp.controller.About"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m">
	<Page
		
		id="page"
		showHeader="true" >
		<customHeader>
			<Bar>
				<contentRight>
					<Button id="edit" text="{i18n>BtnEdit}" enabled="false" press="handleEditPress" />
					<Button id="save" text="{i18n>BtnSave}" type="Emphasized" visible="false" press="handleSavePress" />
					<Button id="cancel" text="{i18n>BtnCancel}" visible="false" press="handleCancelPress" />
					<Button id="back" text="{i18n>BtnBack}" visible="true" press="onBack" />
				</contentRight>
			</Bar>
		</customHeader>
	</Page>
</mvc:View>