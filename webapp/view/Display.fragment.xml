<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
	xmlns:core="sap.ui.core">
	<VBox class="sapUiSmallMargin">
		<f:SimpleForm id="SimpleFormDisplay354wideDual"
			editable="false"
			layout="ResponsiveGridLayout"
			title="{i18n>ContactTitle}"
			labelSpanXL="4"
			labelSpanL="3"
			labelSpanM="4"
			labelSpanS="12"
			adjustLabelSpan="false"
			emptySpanXL="0"
			emptySpanL="4"
			emptySpanM="0"
			emptySpanS="0"
			columnsXL="2"
			columnsL="1"
			columnsM="1"
			singleContainerFullSize="false" >
			<f:content>
				<core:Title text="{i18n>AddressTitle}" />
				<Label text="{i18n>layoutFormName}" />
				<Text id="nameText" text="{SupplierName}" />
				<Label text="{i18n>layoutFormStreet}" />
				<Text text="{Street} {HouseNumber}" />
				<Label text="{i18n>layoutFormZIPCode}" />
				<Text text="{ZIPCode} {City}" />
				<Label text="{i18n>layoutFormCountry}" />
				<Text id="countryText" text="{Country}" />
				<core:Title text="Online" />
				<Label text="{i18n>WebPage}" />
				<Text text="{Url}" />
				<Label text="Twitter" />
				<Text text="{Twitter}" />
			</f:content>
		</f:SimpleForm>
	</VBox>
</core:FragmentDefinition>